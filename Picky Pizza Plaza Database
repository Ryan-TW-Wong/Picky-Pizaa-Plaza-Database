--------------------------------------------CONSTRAINTS-------------------------------------------- 

-- Table 01) MENU 

ALTER TABLE "MENU" 

ADD CONSTRAINT CHK_MENU_PRICE_GT_0 CHECK (PRICE > 0); 

 

-- Table 02) ORDER 

ALTER TABLE "ORDER" 

ADD CONSTRAINT CHK_ORDER_EVENT_DURATION CHECK (EVENT_DURATION <= 8); 

ALTER TABLE "ORDER" 

ADD CONSTRAINT CHK_ORDER_NUM_OF_GUESTS CHECK (NUM_OF_GUESTS <= 50); 

 

-- Table 03) OUTLET 

ALTER TABLE "OUTLET" 

ADD CONSTRAINT CHK_OUTLET_POSTCODE_LENGTH CHECK (LENGTH(POSTCODE) = 5); 

 

--Table 04) STAFF 

ALTER TABLE "STAFF" 

ADD CONSTRAINT CHK_STAFF_TITLE CHECK (TITLE IN ('Mr.', 'Ms.')); 

 

--Table 05) CUSTOMER 

ALTER TABLE "CUSTOMER" 

ADD CONSTRAINT CHK_CUSTOMER_TITLE CHECK (TITLE IN ('Mr.', 'Ms.')); 

 

-- Table 06) VEHICLE 

ALTER TABLE "VEHICLE" 

ADD CONSTRAINT CHK_VEHICLE_MILEAGE_GE_0 CHECK (MILEAGE >= 0); 

 

-- Table 07) LEAVE 

ALTER TABLE "LEAVE" 

ADD CONSTRAINT CHK_LEAVE_APPROVAL_STATUS CHECK (APPROVAL_STATUS IN ('Approved', 'Pending', 'Not Approved')); 

 

-- Table 08) ORDER LINE 

ALTER TABLE "ORDER LINE" 

ADD CONSTRAINT CHK_ORDERLINE_QUANTITY CHECK (QUANTITY > 0); 

 

 

--------------------------------------------FOREIGN KEYS-------------------------------------------- 

-- Foreign Key for "ORDER" table referencing "OUTLET" table 

ALTER TABLE "ORDER" 

ADD CONSTRAINT FK_ORDER_OUTLET 

FOREIGN KEY (OUTLET_NUM) 

REFERENCES "OUTLET" (OUTLET_NUM); 

 

ALTER TABLE "ORDER" 

ADD CONSTRAINT FK_ORDER_CUSTOMER 

FOREIGN KEY (CUSTOMER_NUM) 

REFERENCES "CUSTOMER" (CUSTOMER_NUM); 

 

ALTER TABLE "ORDER" 

ADD CONSTRAINT FK_ORDER_DINEIN 

FOREIGN KEY (PHONE_OPERATOR,ORDER_NUM) 

REFERENCES "DINEIN" (STAFF_NUM,ORDER_NUM); 

 

ALTER TABLE "ORDER" 

ADD CONSTRAINT FK_ORDER_DELIVERY 

FOREIGN KEY (RIDER_NUM,ORDER_NUM) 

REFERENCES "DELIVERY" (STAFF_NUM,ORDER_NUM); 

 

-- Foreign Key for "STAFF" table referencing "OUTLET" table 

ALTER TABLE "STAFF" 

ADD CONSTRAINT FK_STAFF_OUTLET 

FOREIGN KEY (OUTLET_NUM) 

REFERENCES "OUTLET" (OUTLET_NUM); 

 

-- Foreign Key for "VEHICLE" table referencing "OUTLET" table 

ALTER TABLE "VEHICLE" 

ADD CONSTRAINT FK_VEHICLE_OUTLET 

FOREIGN KEY (OUTLET_NUM) 

REFERENCES "OUTLET" (OUTLET_NUM); 

 

ALTER TABLE "DELIVERY" 

ADD CONSTRAINT FK_DELIVERY_ORDER 

FOREIGN KEY (ORDER_NUM) -- Part of composite foreign key 

REFERENCES "ORDER" (ORDER_NUM); 

 

-- Foreign Key for "DELIVERY" table referencing "VEHICLE" table 

ALTER TABLE "DELIVERY" 

ADD CONSTRAINT FK_DELIVERY_VEHICLE  

FOREIGN KEY (VEHICLE_NUM) 

REFERENCES "VEHICLE" (VEHICLE_NUM); 

 

ALTER TABLE "DELIVERY" 

ADD CONSTRAINT FK_DELIVERY_STAFF 

FOREIGN KEY (STAFF_NUM) 

REFERENCES "STAFF" (STAFF_NUM); 

 

-- Foreign Key for "LEAVE" table referencing "STAFF" table 

ALTER TABLE "LEAVE" 

ADD CONSTRAINT FK_LEAVE_STAFF 

FOREIGN KEY (STAFF_NUM) 

REFERENCES "STAFF" (STAFF_NUM); 

 

-- Foreign Key for "ORDER LINE" table referencing "ORDER" table 

ALTER TABLE "ORDER LINE" 

ADD CONSTRAINT FK_ORDERLINE_ORDER 

FOREIGN KEY (ORDER_NUM) 

REFERENCES "ORDER" (ORDER_NUM); 

 

-- Foreign Key for "ORDER LINE" table referencing "MENU" table 

ALTER TABLE "ORDER LINE" 

ADD CONSTRAINT FK_ORDERLINE_MENU 

FOREIGN KEY (ITEM_NUM) 

REFERENCES "MENU" (ITEM_NUM); 

 

-- Foreign Key for "DINEIN" table referencing "ORDER" table 

ALTER TABLE "DINEIN" 

ADD CONSTRAINT FK_DINEIN_ORDER 

FOREIGN KEY (ORDER_NUM) 

REFERENCES "ORDER" (ORDER_NUM); 

 

ALTER TABLE "DINEIN" 

ADD CONSTRAINT FK_DINEIN_STAFF 

FOREIGN KEY (STAFF_NUM) 

REFERENCES "STAFF" (STAFF_NUM); 

 

--------------------------------------------INDEXES-------------------------------------------- 

-- Index for "SUBURB" column in "CUSTOMER" table 

CREATE INDEX IDX_CUSTOMER_SUBURB ON "CUSTOMER" (SUBURB); 

--By creating an index on the "SUBURB" column, the database can efficiently retrieve and group customer data based on their suburbs.  

--This allows for faster and more efficient queries when analyzing customer distribution and helps the business make data-driven decisions about where to open a new outlet to cater to the customer demand in specific areas. 

 

-- Index for "ITEM_NUM" column in "ORDER" table 

CREATE INDEX IDX_ORDER_ITEM_NUM ON "ORDER LINE" (ITEM_NUM); 

--Creating an index on the "ITEM_NUM" column in the "ORDER LINE" table allows you to efficiently analyze and identify the most popular items on the menu.  

--By having this index, the database can quickly retrieve and group order line data based on the item numbers, which helps in aggregating the sales data for each menu item. 

--Knowing which menu items are the most popular enables the business to plan targeted promotions and campaigns around those items.  

--This information can be used to optimize inventory management, ensure the availability of popular items, and design marketing strategies to further boost sales and customer satisfaction. 
