ALTER SESSION SET NLS_DATE_FORMAT='DD/MM/YYYY'; 

 

ALTER TABLE "ORDER LINE" DROP CONSTRAINT FK_ORDERLINE_MENU; 

ALTER TABLE "ORDER" DROP CONSTRAINT FK_ORDER_OUTLET; 

ALTER TABLE "STAFF" DROP CONSTRAINT FK_STAFF_OUTLET; 

ALTER TABLE "VEHICLE" DROP CONSTRAINT FK_VEHICLE_OUTLET; 

ALTER TABLE "ORDER" DROP CONSTRAINT FK_ORDER_DELIVERY; 

ALTER TABLE "ORDER" DROP CONSTRAINT FK_ORDER_DINEIN; 

ALTER TABLE "DELIVERY" DROP CONSTRAINT FK_DELIVERY_ORDER; 

ALTER TABLE "DINEIN" DROP CONSTRAINT FK_DINEIN_ORDER; 

ALTER TABLE "ORDER LINE" DROP CONSTRAINT FK_ORDERLINE_ORDER; 

ALTER TABLE "DELIVERY" DROP CONSTRAINT FK_DELIVERY_STAFF; 

ALTER TABLE "LEAVE" DROP CONSTRAINT FK_LEAVE_STAFF; 

ALTER TABLE "DINEIN" DROP CONSTRAINT FK_DINEIN_STAFF; 

ALTER TABLE "ORDER" DROP CONSTRAINT FK_ORDER_CUSTOMER; 

ALTER TABLE "DELIVERY" DROP CONSTRAINT FK_DELIVERY_VEHICLE; 

 

DROP TABLE "MENU"; 

DROP TABLE "OUTLET"; 

DROP TABLE "CUSTOMER"; 

DROP TABLE "VEHICLE"; 

DROP TABLE "DELIVERY"; 

DROP TABLE "DINEIN"; 

DROP TABLE "ORDER LINE"; 

DROP TABLE "LEAVE"; 

DROP TABLE "STAFF"; 

DROP TABLE "ORDER"; 

 

-- Table 01) MENU 

CREATE TABLE "MENU" 

( 

    ITEM_NUM VARCHAR(5) PRIMARY KEY, 

    DESCRIPTION VARCHAR(40), 

    TYPE_OF_FOOD VARCHAR2(20), 

    PRICE NUMBER(4,2) 

); 

 

