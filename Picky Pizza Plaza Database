-- Table 06) VEHICLE 

CREATE TABLE "VEHICLE" 

( 

    VEHICLE_NUM VARCHAR2(10) PRIMARY KEY, 

    VEHICLE_PRODUCT VARCHAR2(50) NOT NULL, 

    TYPE VARCHAR2(5) NOT NULL, 

    DATE_OF_PURCHASE DATE NOT NULL, 

    LAST_CHECKED_DATE DATE, 

    MILEAGE NUMBER(6), 

    OUTLET_NUM VARCHAR2(3) NOT NULL 

); 

 

