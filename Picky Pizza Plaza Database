-- Table 02) ORDER 

CREATE TABLE "ORDER" 

( 

    ORDER_NUM VARCHAR2(7) PRIMARY KEY, 

    CUSTOMER_NUM NUMBER(6) NOT NULL, 

    ORDER_TIME TIMESTAMP(0) NOT NULL, 

    PHONE_OPERATOR VARCHAR2(5) NOT NULL, 

    RIDER_NUM VARCHAR2(5), 

    OUTLET_NUM VARCHAR2(3) NOT NULL,  

    EVENT_TYPE VARCHAR2(20), 

    EVENT_DURATION NUMBER(2,1), 

    NUM_OF_GUESTS NUMBER(2) 

); 

 

