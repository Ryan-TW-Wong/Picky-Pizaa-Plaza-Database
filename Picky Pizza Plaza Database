-- Table 04) STAFF 

CREATE TABLE "STAFF" 

( 

    STAFF_NUM VARCHAR2(5) PRIMARY KEY, 

    OUTLET_NUM VARCHAR2(3) NOT NULL, 

    TITLE VARCHAR2(3) NOT NULL, 

    FIRST_NAME VARCHAR2(50) NOT NULL, 

    LAST_NAME VARCHAR2(20) NOT NULL, 

    PHONE_NUMBER NUMBER(12) NOT NULL, 

    STREET VARCHAR2(50) NOT NULL, 

    SUBURB VARCHAR2(35) NOT NULL, 

    POSTCODE NUMBER(5) NOT NULL, 

    POSITION VARCHAR2(12) NOT NULL,  

    ENTRY_DATE DATE NOT NULL, 

    DISCIPLINARY_ACTION VARCHAR2(3) NULL 

); 

 

