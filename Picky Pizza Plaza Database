-- Table 03) OUTLET 

CREATE TABLE "OUTLET" 

( 

    OUTLET_NUM VARCHAR2(3) PRIMARY KEY, 

    PHONE_NUM NUMBER(12) NOT NULL, 

    STREET VARCHAR2(50) NOT NULL, 

    SUBURB VARCHAR2(35) NOT NULL, 

    POSTCODE NUMBER(5) NOT NULL 

); 

 

